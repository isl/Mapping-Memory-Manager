    <mappings>
        <mapping>
            <domain>
                <source_node>//COIN</source_node>
                <target_node>
                    <entity>
                        <type>crm:E22_Man-Made_Object</type>
                        <instance_info/>
                        <instance_generator name="LocalTermURI">
                            <arg name="hierarchy" type="constant">COIN</arg>
                            <arg name="term" type="xpath">ID/text()</arg>
                        </instance_generator>
                        <label_generator name="Literal">
                            <arg name="text" type="xpath">ID/text()</arg>
                        </label_generator>
                        <additional>
                            <relationship>crm:P52_has_current_owner</relationship>
                            <entity>
                                <type>crm:E40_Legal_Body</type>
                                <instance_info>
                                    <constant>OEAW</constant>
                                </instance_info>
                                <instance_generator name="URIorUUID">
                                    <arg name="text" type="constant">http://www.oeaw.ac.at/</arg>
                                </instance_generator>
                                <label_generator name="Literal">
                                    <arg name="text" type="constant">Österreichische Akademie der Wissenschaften</arg>
                                    <arg name="language" type="constant">de</arg>
                                </label_generator>
                                <label_generator name="Literal">
                                    <arg name="text" type="constant">Austrian Academy of Science</arg>
                                    <arg name="language" type="constant">en</arg>
                                </label_generator>
                            </entity>
                        </additional>
                        <additional>
                            <relationship>crm:P50_has_current_keeper</relationship>
                            <entity>
                                <type>crm:E40_Legal_Body</type>
                                <instance_info>
                                    <constant>OEAW</constant>
                                </instance_info>
                                <instance_generator name="URIorUUID">
                                    <arg name="text" type="constant">http://www.oeaw.ac.at/</arg>
                                </instance_generator>
                            </entity>
                        </additional>
                        <additional>
                            <relationship>crm:P2_has_type</relationship>
                            <entity>
                                <type>crm:E55_Type</type>
                                <type>http://www.w3.org/2004/02/skos/core#Concept</type>
                                <instance_info>
                                    <constant>coins</constant>
                                </instance_info>
                                <instance_generator name="ARIADNEtermURI">
                                    <arg name="hierarchy" type="constant">concepts</arg>
                                    <arg name="term" type="constant">coins</arg>
                                </instance_generator>
                                <label_generator name="prefLabel">
                                    <arg name="text" type="constant">coins</arg>
                                </label_generator>
                            </entity>
                        </additional>
                        <additional>
                            <relationship>crm:P46i_forms_part_of</relationship>
                            <entity>
                                <type>crm:E78_Collection</type>
                                <instance_info>
                                    <constant>dFMRO</constant>
                                </instance_info>
                                <instance_generator name="LocalTermURI">
                                    <arg name="hierarchy" type="constant">collection</arg>
                                    <arg name="term" type="constant">dFMRO</arg>
                                </instance_generator>
                                <label_generator name="Constant">
                                    <arg name="text" type="constant">Coin Collection dFMRO</arg>
                                    <arg name="language" type="constant">en</arg>
                                </label_generator>
                            </entity>
                        </additional>
                    </entity>
                </target_node>
            </domain>
            <link>
                <path>
                    <source_relation>
                        <relation>ID</relation>
                    </source_relation>
                    <target_relation>
                        <relationship>crm:P1_is_identified_by</relationship>
